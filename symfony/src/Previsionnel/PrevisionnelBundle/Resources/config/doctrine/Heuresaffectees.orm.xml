<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Previsionnel\PrevisionnelBundle\Entity\Heuresaffectees" table="heuresaffectees">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idutilisateur" type="integer" column="idUtilisateur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idcours" type="integer" column="idCours" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idstatut" type="integer" column="idStatut" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbheures" type="float" column="nbHeures" precision="10" scale="0" nullable="false"/>
    <field name="annee" type="string" column="annee" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
